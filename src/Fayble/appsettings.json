{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},	
	"AllowedHosts": "*",
	"Authentication": {
		"TokenAudience": "https://fayble",
		"TokenIssuer": "https://fayble"
	},
	"FaybleApi": {
		//"baseUrl": "https://metadata.fayble.org"
		"baseUrl": "https://fayble-metadata-api-staging.azurewebsites.net"
	},
	"IpRateLimiting": {
		"EnableEndpointRateLimiting": true,
		"StackBlockedRequests": false,
		"RealIpHeader": "X-Real-IP",
		"ClientIdHeader": "X-ClientId",
		"HttpStatusCode": 429,
		"GeneralRules": [
			{
				"Endpoint": "*:/api/authentication/login",
				"Period": "60s",
				"Limit": 10
			}
		]
	}
}
